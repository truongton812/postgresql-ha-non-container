# Điền đúng cấu trúc [name_server] ansible_host=[IP-address] nếu muốn khởi tạo file hosts

[servers]
node1  ansible_host=192.168.179.201 ansible_ssh_user=tvcuongnd ansible_ssh_pass="123abc@A" ansible_become_password="123abc@A"
node2   ansible_host=192.168.179.202 ansible_ssh_user=tvcuongnd ansible_ssh_pass="123abc@A" ansible_become_password="123abc@A"
node3   ansible_host=192.168.179.203 ansible_ssh_user=tvcuongnd ansible_ssh_pass="123abc@A" ansible_become_password="123abc@A"



